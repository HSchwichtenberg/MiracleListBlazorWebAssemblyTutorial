<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

 <!--notwendige Frameworkeinstellungen-->
 <PropertyGroup>
  <TargetFramework>net5.0</TargetFramework>
 </PropertyGroup>

 <!--optionale Anwendungseinstellungen-->
 <PropertyGroup>
  <Description>Blazor WebAssembly-Version des MiracleList Web Frontends</Description>
  <Copyright>Dr. Holger Schwichtenberg, www.IT-Visions.de 2018-2020</Copyright>
  <Authors>Dr. Holger Schwichtenberg, www.IT-Visions.de</Authors>
  <Company>www.IT-Visions.de</Company>
  <Product>MiracleList</Product>
  <Version>0.4.0.0</Version>
  <RootNamespace>Web</RootNamespace>
  <StartupObject>Web.Program</StartupObject>
  <AssemblyVersion>0.4.0.0</AssemblyVersion>
  <FileVersion>0.4.0.0</FileVersion>
 </PropertyGroup>
 <ItemGroup>
   <None Remove="Pages\Login.razor.css" />
 </ItemGroup>

 <ItemGroup>
  <BlazorWebAssemblyLazyLoad Include="MiracleListRCL" />
 </ItemGroup>

 <!--Unterdrückung einiger Warnungen-->
 <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
  <NoWarn>1701;1702;1998</NoWarn>
 </PropertyGroup>
 <ItemGroup>
   <Content Include="Pages\Login.razor.css">
     <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
     <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
   </Content>
 </ItemGroup>

 <ItemGroup>
  <!--Standardreferenzen für BW-->
  <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="5.0.0-rc.1.20451.17" />
  <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="5.0.0-rc.1.20451.17" PrivateAssets="all" />
  <PackageReference Include="System.Net.Http.Json" Version="5.0.0-rc.1.20451.14" />
  <PackageReference Include="Microsoft.AspNetCore.Components.Authorization" Version="5.0.0-rc.1.20451.17" />
 </ItemGroup>

 <ItemGroup>
  <ProjectReference Include="..\BO\BO.csproj" />
  <ProjectReference Include="..\MiracleListAPI_Proxy\MiracleListAPI_Proxy.csproj" />
  <ProjectReference Include="..\MiracleListRCL\MiracleListRCL.csproj" />
  <ProjectReference Include="..\MiracleListUtil\MiracleListUtil.csproj" />
 </ItemGroup>
</Project>